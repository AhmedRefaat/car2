<!DOCTYPE html>
<html>
<head>
  <title>Ccar</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<% if session[:user_id]%>
		<ul>
			<li><%= link_to "Add Used Car", new_ocar_path%></li>
			<li><%= link_to "Add New Car", new_ncar_path%></li>
		</ul>
		<%= button_to "logout", logout_path, :method => "delete"%>
		<%else%>
		<%= button_to "login", login_path, :method => "get"%>
		<%= button_to "New User", new_user_path, :method => "get"%>
	<%end%>
<%= yield %>

</body>
</html>
